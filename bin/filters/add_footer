#!/usr/bin/awk -f

{ print }

END {
  if ( ENVIRON["TAIL_CONTENT"] != "" ) {
    print ""
    print ENVIRON["TAIL_CONTENT"]
  }
  print ""
  print "#"
  print "# Wrap up"
  print "#"
  print ""
  print "# Tell Zephyr this plugin is loaded."
  print "zstyle ':zephyr:plugin:"PLUGIN_NAME"' loaded 'yes'"
}
